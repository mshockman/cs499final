<html lang="en">
<head>
    <title></title>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>

    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>

    <link type="text/css" rel="stylesheet" href="{{ request.static_url('final499:static/css/style.css') }}" />
    <script type="text/javascript" src="{{ request.static_url('final499:static/js/cs499.bundle.js') }}"></script>

    <style type="text/css">
        #stock-table {
            table-layout: fixed;
        }

        #stock-table th {
            white-space: nowrap;
            width: 200px;
            overflow: hidden;
        }

        #stock-table [data-column="cb"] {
            width: 50px;
        }

        #stock-table [data-column="ticker"] {
            width: 100px;
        }

        #stock-table [data-column="sector"] {
            width: 100px;
        }

        #stock-table [data-column="200_day_simple_moving_average"] {
            width: 300px;
        }

        #stock-table [data-column="industry"] {
            width: 300px;
        }

        #stock-table [data-column="50_day_simple_moving_average"] {
            width: 400px;
        }
    </style>
</head>
<body>
    <div class="main-menu">
        <a href="{{ request.route_url('home') }}">View Stocks</a>
        <a href="{{ request.route_url('stock') }}">Create Stock</a>
    </div>
    {% block content %}{% endblock %}
</body>
</html>